{
    "manifest_version": 2,
    "name": "moodle class selecter",
    "description": "表示する授業数の制限",
    "version": "0.1",
    "icons": {
        "16": "images/icon_16.png",
        "32": "images/icon_32.png",
        "128": "images/icon_128.png"
    },
    "browser_action": {
        "default_popup": "main.html",
        "default_icon": "images/icon_m.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.center.kobe-u.ac.jp/*"
            ],
            "js": [
                "main.js"
            ],
            "run_at": "document_end"
        }
    ]
}